  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/moment-timezone": "^0.5.9",
    "@types/node": "^10.12.10",
    "@types/react": "^16.7.7",
    "@types/react-color": "^2.14.0",
